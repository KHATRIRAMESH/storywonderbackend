    "migrate": "ts-node scripts/migrate.ts",
    "migrate-to-gcs": "ts-node scripts/migrate-to-gcs.ts",
    "migrate-to-gcs:dry-run": "ts-node scripts/migrate-to-gcs.ts --dry-run",
